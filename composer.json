{
    "name": "axelbest/sendbird-client",
    "description": "Sendbird API handler for Symfony",
    "type": "symfony-bundle",
    "license": "proprietary",

    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Axelbest\\SendbirdClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axelbest\\SendbirdClient\\Tests\\": "src/Tests/"
        }
    },
    "authors": [
        {
            "name": "Emil Krajewski",
            "email": "axelbest@o2.pl"
        },
        {
            "name": "Marcin Harasim",
            "email": "marcin.harasim@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.1",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/http-client": "5.3.*",
        "symfony/http-foundation": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/security-core": "5.3.*",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3",
        "symfony/var-dumper": "5.3.*"
    },
    "config": {
        "sort-packages": true
    }
}
